{
  "name": "tecnodesignc/adminlte-theme",
  "type": "encore-theme",
  "description": "An EncoreCMS administration theme",
  "keywords": [
    "encore-cms"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Marcos Rativa",
      "email": "info@tecnodesign.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.0",
    "composer/installers": "^2.1"
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan stylist:publish"
    ],
    "post-update-cmd": [
      "php artisan stylist:publish"
    ]
  },

  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
